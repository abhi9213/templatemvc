
@{
    ViewBag.Title = "showtable";
}



                                    <h2>showtable</h2>
                                    @model List<templatemvc.Models.mymodelClass1>
                                    <head>
                                        <!--<link href="../css/bootstrap.min.css" rel="stylesheet">-->
                                        <!-- MetisMenu CSS -->
                                        <!--<link href="../css/metisMenu.min.css" rel="stylesheet">-->
                                        <!-- DataTables CSS -->
                                        <!--<link href="../css/dataTables/dataTables.bootstrap.css" rel="stylesheet">-->
                                        <!-- DataTables Responsive CSS -->
                                        <!--<link href="../css/dataTables/dataTables.responsive.css" rel="stylesheet">-->
                                        <!-- Custom CSS -->
                                        <!--<link href="../css/startmin.css" rel="stylesheet">-->
                                        <!-- Custom Fonts -->
                                        <!--<link href="../css/font-awesome.min.css" rel="stylesheet" type="text/css">-->
                                        <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" />


                                        <link href="~/Content/css/metisMenu.min.css" rel="stylesheet" />
                                        <link href="~/Content/css/dataTables/dataTables.bootstrap.css" rel="stylesheet" />

                                        <link href="~/Content/css/dataTables/dataTables.responsive.css" rel="stylesheet" />

                                        <link href="~/Content/css/startmin.css" rel="stylesheet" />
                                        <link href="~/Content/css/font-awesome.min.css" rel="stylesheet" />






                                    </head>

                                    <body>
                                        <div id="wrapper">

                                            <div id="page-wrapper">
                                                <div class="container-fluid">
                                                    <div class="row">
                                                        <div class="col-lg-12">
                                                            <h1 class="page-header">Tables</h1>
                                                        </div>
                                                        <!-- /.col-lg-12 -->
                                                    </div>
                                                    <!-- /.row -->
                                                    <div class="row">
                                                        <div class="col-lg-12">
                                                            <div class="panel panel-default">
                                                                <div class="panel-heading">
                                                                    DataTables Advanced Tables
                                                                </div>
                                                                <!-- /.panel-heading -->
                                                                <div class="panel-body">
                                                                    <div class="table-responsive">
                                                                        <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                                                                            <thead>
                                                                                <tr>
                                                                                    <th>ID</th>
                                                                                    <th>USER NAME</th>
                                                                                    <th>EMAIL ID</th>
                                                                                    <th>PASSWORD</th>
                                                                                    <th>NAME</th>
                                                                                    <th>EDIT</th>
                                                                                    <th>DELETE</th>
                                                                                </tr>
                                                                            </thead>
                                                                            <tbody>
                                                                                @foreach (var item in Model)
                                                                                {
                                                                                    <tr class="odd gradeX">



                                                                                        <td>@item.id</td>
                                                                                        <td>@item.username</td>
                                                                                        <td>@item.email</td>
                                                                                        <td>@item.pass</td>
                                                                                        <td>@item.name</td>
                                                                                        <td><a href="@Url.Action("addata", "Home", new {item.id})" i class="fa fa-edit "></a></td>

                                                                                        <td><a href="@Url.Action("addata", "Home", new {item.id})" i class="fa fa-trash "></a></td>
                                                                                    </tr>

                                                                                }


                                                                                </tr>
                                                                            </tbody>
                                                                        </table>
                                                                    </div>
                                                                    <!-- /.table-responsive -->
                                                                </div>
                                                                <!-- /.panel-body -->
                                                            </div>
                                                            <!-- /.panel -->
                                                        </div>
                                                        <!-- /.col-lg-6 -->
                                                    </div>
                                                    <!-- /.row -->
                                                </div>
                                                <!-- /.container-fluid -->
                                            </div>
                                            <!-- jQuery -->
                                            <!--<script src="../js/jquery.min.js"></script>-->
                                            <!-- Bootstrap Core JavaScript -->
                                            <!--<script src="../js/bootstrap.min.js"></script>-->
                                            <!-- Metis Menu Plugin JavaScript -->
                                            <!--<script src="../js/metisMenu.min.js"></script>-->
                                            <!-- DataTables JavaScript -->
                                            <!--<script src="../js/dataTables/jquery.dataTables.min.js"></script>
                                            <script src="../js/dataTables/dataTables.bootstrap.min.js"></script>-->
                                            <!-- Custom Theme JavaScript -->
                                            <!--<script src="../js/startmin.js"></script>-->
                                            <script src="~/Scripts/js/jquery.min.js"></script>
                                            <script src="~/Scripts/js/bootstrap.min.js"></script>
                                            <script src="~/Scripts/js/metisMenu.min.js"></script>

                                            <script src="~/Scripts/js/dataTables/jquery.dataTables.min.js"></script>
                                            <script src="~/Scripts/js/dataTables/dataTables.bootstrap.min.js"></script>
                                            <script src="~/Scripts/js/startmin.js"></script>
                                    </body>
